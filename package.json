{
  "name": "EventsManager",
  "version": "0.1.0",
  "description": "A simple full-featured website for managing events centers and scheduling events. https://events-manager-io.herokuapp.com/",
  "main": "server/bin/server.js",
  "scripts": {
    "start": "node server/server.js --exec babel-node server/server.js",
    "server:watch": "nodemon server/server.js --exec babel-node server/server.js",
    "compile:sequelize": "babel server/migrations --out-dir server/bin/migrations & babel server/seeders --out-dir server/bin/seeders & babel server/models --out-dir server/bin/models",
    "webpack:watch": "webpack-dev-server --config webpack/client.dev.js --content-base client/public --inline --hot",
    "build": "npm run build:client:prod",
    "test": "npm run test:api & npm run test:react",
    "test:api": "npm run test:api:demigrate && npm run test:api:migrate && cross-env NODE_ENV=test PORT=4200 nyc --reporter=html --reporter=lcov --reporter=text mocha --timeout 250000 ./tests/api/*.js --compilers js:babel-core/register --exit",
    "test:e2e": "cross-env NODE_ENV=test nightwatch tests/e2e/bin/test.js --config tests/e2e/config/nightwatch.conf.js",
    "test:react": "cross-env NODE_ENV=test jest tests/react/bin/test.js",
    "test:api:migrate": "cross-env NODE_ENV=test sequelize db:migrate",
    "test:api:demigrate": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "build:server": "cross-env NODE_ENV=development webpack --config webpack/server.dev.js --progress",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config webpack/client.dev.js --progress",
    "build:client:prod": "cross-env NODE_ENV=production webpack --config webpack/client.dev.js --progress",
    "build:server:watch": "cross-env NODE_ENV=development webpack --config webpack/server.dev.js --watch --progress",
    "build:client:dev:watch": "cross-env NODE_ENV=development webpack --config webpack/client.dev.js --watch --progress",
    "build:client:prod:watch": "cross-env NODE_ENV=production webpack --config webpack/client.prod.js --watch --progress",
    "sass:watch": "sass --watch ./template/scss:./template/css",
    "lint": "eslint --ext .jsx,.js ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appcypher/eventsmanager"
  },
  "keywords": [],
  "author": "Steve Akinyemi",
  "license": "Apache v2.0",
  "bugs": {
    "url": "git+https://github.com/appcypher/eventsmanager"
  },
  "homepage": "",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.2",
    "morgan": "^1.9.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.11",
    "sequelize-cli": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.1",
    "nyc": "^11.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-node-externals": "^1.6.0"
  },
  "bundledDependencies": [],
  "nyc": {
    "exclude": [
      "coverage/",
      "webpack/",
      "template/"
    ]
  }
}
